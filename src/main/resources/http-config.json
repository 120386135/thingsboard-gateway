      "converters": [
        {
          "deviceNameJsonExpression": "${$.DevEUI_uplink.DevEUI}",
          "attributes": [
            {
              "type": "string",
              "key": "SubBand",
              "value": "${$.DevEUI_uplink.SubBand}"
