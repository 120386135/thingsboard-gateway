
{
"broker": {
    "host":"127.0.0.1",
    "port": 1883,
    "clientId": "ThingsBoard_gateway_debug",
    "security": {
      "type": "anonymous"
    }
  },
  "mapping": [
    {
      "topicFilter": "sensor/data",
      "converter": {
        "type": "json",
        "deviceNameJsonExpression": "${Name}",
        "deviceTypeJsonExpression": "${Type}",

        "timeseries": [
            {
                "type": "integer",
                "key": "temperature",
                "value": "${temp}"
            }
        ],
        "attributes": [
            {
                "type": "string",
                "key": "model",
                "value": "${model}"
            }
        ]
     }
    }
  ],
  "connectRequests": [
    {
      "topicFilter": "sensor/connect",
      "deviceNameJsonExpression": "${Name}"
    }
  ]
}

