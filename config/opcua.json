{
  "server": {
    "name": "OPC-UA Default Server",
    "uri": "",
    "url": "localhost:4840/freeopcua/server/",
    "scanPeriodInMillis": 10000,
    "timeoutInMillis": 5000,
    "security": "Basic128Rsa15",
    "identity": {
      "type": "anonymous"
    },
    "keystore": {
      "type": "PKCS12",
      "location": "example.pfx",
      "fileContent": "",
      "password": "secret",
      "alias": "gateway",
      "keyPassword": "secret"
    },
    "mapping": [
      {
        "deviceNodePattern": "MyObject1\\.MyVariable\\d+$",
        "deviceNamePattern": "Device ${_System._DeviceId}",
        "attributes": [
          {
            "key": "Tag1",
            "type": "string",
            "value": "${Tag1}"
          }
        ],
        "timeseries": [
          {
            "key": "Tag2",
            "type": "long",
            "value": "${Tag2}"
          }
        ]
      }
    ]
  }

}