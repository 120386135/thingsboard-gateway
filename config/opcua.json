{
  "server": {
    "name": "OPC-UA Default Server",
    "url": "localhost:4840/freeopcua/server/",
    "scanPeriodInMillis": 10000,
    "timeoutInMillis": 5000,
    "security": "Basic128Rsa15",
    "identity": {
      "type": "anonymous"
    },
    "keystore": {
      "type": "PKCS12",
      "location": "example.pfx",
      "fileContent": "",
      "password": "secret",
      "alias": "gateway",
      "keyPassword": "secret"
    },
    "mapping": [
      {
        "deviceNodePattern": "MyObject\\d+",
        "deviceNamePattern": "Device ${MyVariable22}",
        "attributes": [
          {
            "key": "Tag1",
            "path": "${MyVariable1}"
          }
        ],
        "timeseries": [
          {
            "key": "Tag3",
            "path": "${Tag3}"
          },
          {
            "key": "Tag2",
            "path": "${MyVariable3}"
          }
        ]
      }
    ]
  }

}